{
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",
    "outDir": "dist",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Bundler",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["src/**/*.ts", "demo/*.ts"],
  "exclude": ["vite.config.ts"]
}
